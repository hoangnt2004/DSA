#include<bits/stdc++.h>
using namespace std;

int main(){
    int t;
    cin >> t;
    while(t--){
        int k;
        string s;
        cin >> k >> s;
        cout << k <<" ";
        if(next_permutation(s.begin(), s.end())){
            cout << s <<endl;
        }else{
            cout << "BIGGEST" <<endl;
        }
    }
}
